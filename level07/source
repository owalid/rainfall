char s[68];

void m()
{
	time_t	time;

	time(&time);
	printf("%s - %d\n", s, time);
}

struct s_var {
	int	nb;
	char	*s;
}

int main()
{
	struct s_var var;
	struct s_var var;
	FILE *fd;

	var1 = malloc(8);
	var1->nb = 1;
	var1->s = malloc(8);
	var2 = malloc(8);
	var2->nb = 2;
	var2->s = malloc(8);

	strcpy(var1->s, argv[1]);
	strcpy(var2->s, argv[2]);

	fd = fopen("/home/user/level8/.pass", "r");
	fgets(&s, 68, fd)

	puts("~~\n");
}
