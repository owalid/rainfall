class N()
{
public:
	char*	str;
	int		nombre;

	N(int n)
	{
		this->nombre = n;
	}

	int setAnnotation(void *s1)
	{
		len = strlen(s1);
		memcpy(this->str, s1, len)
	}

	int operator+(N *n)
	{
		return this->nombre + n->nombre;
	}

	int operator-(N *n, void *arg2)
	{
		return this->nombre - n->nombre;
	}
}

int main()
{
	if (argv[] == 1)
		exit(1);

	F *a, *b;
	a = new N(5);
	b = new N(6)
	a->setAnnotation(argv[1]);
	return a(a, b);
}
